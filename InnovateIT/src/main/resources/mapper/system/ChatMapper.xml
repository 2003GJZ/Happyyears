<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.system.mapper.ChatMapper">
    <select id="selectChat" resultType="java.lang.Long">
        select id
        from chat
        where a_id = #{aId}
          and b_id = #{bId}
    </select>
</mapper>
